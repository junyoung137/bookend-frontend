# .env.production
# Vercel 환경 변수 (Vercel 대시보드에서 설정)
# 주의: 이 파일을 git에 커밋하지 마세요 (.gitignore에 추가)

# 백엔드 API URL - Vercel에서 직접 설정하세요
NEXT_PUBLIC_API_URL=https://your-backend-url.com

# 기능 활성화
NEXT_PUBLIC_ENABLE_LLM=true
NEXT_PUBLIC_ENABLE_SPELLCHECK=true
NEXT_PUBLIC_ENABLE_DEBUG=false
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# LLM 설정
NEXT_PUBLIC_LLM_PROVIDER=huggingface
NEXT_PUBLIC_LLM_MODEL=Qwen/Qwen2.5-7B-Instruct
NEXT_PUBLIC_LLM_TIMEOUT=30000
NEXT_PUBLIC_LLM_MAX_RETRIES=3

# 캐시 설정
NEXT_PUBLIC_CACHE_ENABLED=true
NEXT_PUBLIC_CACHE_TTL=3600000